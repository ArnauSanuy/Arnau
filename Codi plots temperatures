import numpy as np
import matplotlib.pyplot as plt
import math


# Constants basiques
alpha = 0.3
beta = 0.5
c1 = 0.5
c3 = 0.3
delta = 0.0005
f= 1


# Exemple de definicio
def list_numbers_with_intervals(start, end, interval):
    return list(range(start, end + 1, interval))

# Ejemplo de uso
start = 0.95
end = 1.4
interval = 0.01


# Valors diferents per poder calcular xm i theta
Ac1 = np.linspace(0.94,1.4,51)
Ac2 = np.linspace(0.96,1.3,51)
e = 1
B = 1
c = 3e10
me = 510998.95/(c**2)
#vb = ((e*B)/(2*math.pi*me*c))
electro = 4.8032*10**(-10)
massa_electro = 9.1094*10**(-28)
vb = (electro*B)/(2*math.pi*massa_electro*c)
#print(vb)
#k = 8.617333262*10**(-5)
kTe = np.linspace(50000, 1000000, 51)
theta = kTe/(me*c**2)
#print(theta)
v = np.logspace(12,21,51)
logv = []
xm = ((2*v)/(3*vb*theta**2))
print(xm)

for i in range(51):
    info = math.log10(v[i])
    logv.append(info)


# 3 casos diferents segons quin alphac tenim o derivada de massa
rmin = 3
rmax = 1000
m = [10, 1e5, 1e7, 1e9]
mp1 = np.linspace(9*10**(-5), 1*10**(-7),51)
mp2 = np.linspace(3*10**(-4), 1*10**(-4),51)
mp3 = np.linspace(1*10**(-4), 8*10**(-3),51)
tau1 = (23.87*mp1)*(alpha/0.3)**(-1)*(c1/0.5)**(-1)*(rmin/3)**(-1/2)
tau2 = (23.87*mp2)*((alpha/0.3)**(-1))*((c1/0.5)**(-1))*((rmin/3)**(-1/2))
tau3 = (23.87*mp3)*(alpha/0.3)**(-1)*(c1/0.5)**(-1)*(rmin/3)**(-1/2)
alphac1 = np.linspace(1.01,10,51)
alphac2 = np.linspace(0.50,1,51)
alphac3 = np.linspace(1.01,10,51)


logxm = []
xm_nova = []

for i in range(51):
    cas1 = 3.6+0.25*math.log10(mp1[i])
    logxm.append(cas1)

for i in range(51):
    llista = 10**(logxm[i])
    xm_nova.append(llista)

#print(logxm)
print(xm_nova)

nova_alpha1 = 1-alphac1
nova_alpha2 = 1-alphac2
nova_alpha3 = 1-alphac3
#print(nova_alpha1)

logMp1 = []

for i in range(51):
    masa = math.log10(mp1[i])
    logMp1.append(masa)

logMp2 = []

for i in range(51):
    masa = math.log10(mp2[i])
    logMp2.append(masa)

logMp3 = []

for i in range(51):
    masa = math.log10(mp3[i])
    logMp3.append(masa)

#print(logMp)


# Son les tres temperatures
#Te1 = (1.1/Ac1)*(2000*delta)**(1/7)*(xm/300)**(-3/7)*(alpha/0.3)**(3/14)*((1-beta)/0.5)**(-1/14)*(c1/0.5)**(3/14)*(c3/0.3)**(-1/14)*(rmin/3)**(3/28)*m**(1/14)*mp1**(-1/14)

#Te2 = 0.744*((4*tau2**(-1/alphac2)-3)**(1/2)-1)

#Te3 = (2.7/Ac2)*(xm/1000)**(-2/5)*(alpha/0.3)**(-3/50)*(beta/0.5)**(3/25)*((1-beta)/0.5)**(-1/5)*(c1/0.5)**(-3/50)*(c3/0.3)**(-3/50)*(rmin/3)**(1/10)*m**(3/50)*mp3**(3/50)


# Ara ja es poden fer els plots 

Te = []


for j in m:
    for i in range(51):
        cas = (1.1/Ac1[i])*((xm[i]/300)**(-3/7))*(((1-beta)/0.5)**(-1/14))*(j**(1/14))*(mp1[i]**(-1/14))
        Te.append(cas)
    

massa1 = []
massa2 = []
massa3 = []
massa4 = []

for i in range(51):
    cas = ((1.1e9)/Ac1[i])*((xm[i]/300)**(-3/7))*(m[0]**(1/14))*(mp1[i]**(-1/14))
    massa1.append(cas)


for i in range(51):
    cas = ((1.1e9)/Ac1[i])*((xm[i]/300)**(-3/7))*(m[1]**(1/14))*(mp1[i]**(-1/14))
    massa2.append(cas)


for i in range(51):
    cas = ((1.1e9)/Ac1[i])*((xm[i]/300)**(-3/7))*(m[2]**(1/14))*(mp1[i]**(-1/14))
    massa3.append(cas)


for i in range(51):
    cas = ((1.1e9)/Ac1[i])*((xm[i]/300)**(-3/7))*(m[3]**(1/14))*(mp1[i]**(-1/14))
    massa4.append(cas)


plt.plot(logv,massa1, label="m=10")
plt.plot(logv,massa2, label="$m=10^5$")
plt.plot(logv,massa3, label="$m=10^7$")
plt.plot(logv,massa4, label="$m=10^9$")
plt.xlabel(r'$\nu$ Hz')
plt.ylabel('Te')
plt.title('Relacio temperatures')
plt.legend()
plt.show()


T2 = []

for i in range(51):
    cas2 = (0.744e9)*(((4*tau2[i]**(-1/alphac2[i]))-3)**(1/2)-1)
    T2.append(cas2)

#print(T2)

plt.plot(logv,T2)
plt.xlabel('$\nu$ Hz')
plt.ylabel('Te')
plt.title('Relacio temperatures')
plt.show()


T3_masa1 = []
T3_masa2 = []
T3_masa3 = []
T3_masa4 = []


for i in range(51):
    cas3 = ((2.7e9)/Ac2[i])*((xm[i]/1000)**(-2/5))*(((1-beta)/0.5)**(-1/5))*(m[0]**(3/50))*(mp3[i]**(3/50))
    T3_masa1.append(cas3)

for i in range(51):
    cas3 = ((2.7e9)/Ac2[i])*((xm[i]/1000)**(-2/5))*(((1-beta)/0.5)**(-1/5))*(m[1]**(3/50))*(mp3[i]**(3/50))
    T3_masa2.append(cas3)

for i in range(51):
    cas3 = ((2.7e9)/Ac2[i])*((xm[i]/1000)**(-2/5))*(((1-beta)/0.5)**(-1/5))*(m[2]**(3/50))*(mp3[i]**(3/50))
    T3_masa3.append(cas3)

for i in range(51):
    cas3 = ((2.7e9)/Ac2[i])*((xm[i]/1000)**(-2/5))*(((1-beta)/0.5)**(-1/5))*(m[3]**(3/50))*(mp3[i]**(3/50))
    T3_masa4.append(cas3)


plt.plot(logv,T3_masa1, label="m=10")
plt.plot(logv,T3_masa2, label="$m=10^5$")
plt.plot(logv,T3_masa3, label="$m=10^7$")
plt.plot(logv,T3_masa4, label="$m=10^9$")
plt.xlabel('$\nu$ Hz')
plt.ylabel('Te')
plt.title('Relacio temperatures')
plt.legend()
plt.show()


